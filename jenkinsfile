pipeline{
    agent {
        label 'orange-pi'
    }
    
    stages{
        stage('clean'){
            steps{
                cleanWs()
            }
        }
     
        stage('Checkout from SCM'){
            steps{
                git branch: 'main', url : 'https://github.com/darktang3nt69/complete-production-e2e-pipeline.git'
            }
            
        }
    }
}